{
  "name": "chrome-extension-template",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "bun@1.0.0",
  "engines": {
    "node": "20.*",
    "bun": "1.*"
  },
  "scripts": {
    "dev": "bunx --bun vite --host --mode development",
    "watch": "bunx --bun vite build --watch --mode development",
