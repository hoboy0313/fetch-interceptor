    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@tailwindcss/vite": "^4.1.3",
    "@types/chrome": "^0.0.254",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.23.0",
